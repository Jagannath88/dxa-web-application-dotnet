@model Footer
<footer class="page-footer">
    @foreach (Region region in Model.Regions.Values)
    {
        @ViewBag.Renderer.Render(region, Html, 12, new List<string>{"Links"})
    }
    <hr />
    @if (Model.Regions.ContainsKey("Links"))
    {
        @ViewBag.Renderer.Render(Model.Regions["Links"], Html)
    }
</footer>