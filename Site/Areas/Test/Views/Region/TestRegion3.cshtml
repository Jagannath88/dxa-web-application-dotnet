@model RegionModel
<div class="region" @Html.DxaRegionMarkup()>
    <h3>Rendered @Model with Region View "Test:TestRegion3"</h3>
    
    @if (Model.Regions.Count > 0)
    {
        <h4>Nested Regions</h4>
        <div>
            @Html.DxaRegions()
        </div>
    }

    <h4>Entities</h4>
    <div>
        @Html.DxaEntities()
    </div>
</div>
